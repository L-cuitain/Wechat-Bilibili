    <navigator class="logo">
        <image class="logo_img" style="width:140rpx;height:60rpx;" src="../../icon/logo2.png"></image>
    </navigator>
    <!-- 搜索 -->
    <view class="search_icon">
        <image class="search_img" style="width:40rpx;height:40rpx;" src="../../icon/search.png"></image>
